    <PathDelim Value="\"/>
    <SearchPaths>
      <IncludeFiles Value="..\..\..\..\Source\Core\"/>
      <OtherUnitFiles Value="..\..\..\..\Source\Core\;..\..\..\..\Source\Brokers\;..\..\Units\"/>
    </SearchPaths>
    <CodeGeneration>
      <Generate Value="Faster"/>
    </CodeGeneration>
    <Linking>
      <Options>
        <Win32>
          <GraphicApplication Value="True"/>
        </Win32>
      </Options>
    </Linking>
    <Other>
      <CompilerPath Value="$(CompPath)"/>
    </Other>
  </CompilerOptions>
</CONFIG>
